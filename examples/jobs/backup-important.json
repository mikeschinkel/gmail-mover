
{
   "name": "Backup Important Emails",
   "description": "Backup emails with important label to secondary account",
   "source": {
      "email": "work@company.com",
      "labels": ["important", "work/contracts"],
      "date_filter": {
         "after": "2024-01-01"
      }
   },
   "destination": {
      "email": "backup@personal.com",
      "apply_label": "work-backup",
      "preserve_labels": true
   },
   "options": {
      "delete_after_move": false,
      "dry_run": false,
      "log_level": "debug"
   }
}